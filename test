<script>
    $(document).ready(function () {
        $('.clearfix').addClass('hidden');
        //$('.banner-page-section').removeClass('home');
        $('#content').addClass('homeContent');
    });
    $('.cardHome').on('click', function (event) {

        var url = '@System.Configuration.ConfigurationManager.AppSettings["Baseurl"]' + "@url";

        var token = $('input[name="__RequestVerificationToken"]', '#__AjaxAntiForgeryForm').val();
        debugger
        $.ajax({
            type: "POST",
            url: url,
            data: {
                "__RequestVerificationToken": token
            },
            success: function (response) {
                if (response.status == 401) {
                    window.location.href = "@System.Configuration.ConfigurationManager.AppSettings["Baseurl"]/Home/Index";
                }
                else {
                    $('.clearfix').removeClass('hidden');
                    //$('.banner-page-section').removeClass('home');
                    $('#content').removeClass('homeContent');
                    $('body').html(response);
                    //$('#content').html(response);
                }
            },
            error: function (arg) {
                if (arg.status == 401) {
                    window.location.href = "@System.Configuration.ConfigurationManager.AppSettings["Baseurl"]/Home/Index";
                }
            }
        });
    });
</script>
